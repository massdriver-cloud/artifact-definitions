{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Qdrant specs",
  "description": "Common metadata for Qdrant databases.",
  "examples": [
    {
      "version": "0.6.1"
    }
  ],
  "required": [
    "version"
  ],
  "properties": {
    "version": {
      "type": "string",
      "title": "Version",
      "description": "The Qdrant database version.",
      "default": "",
      "examples": [
        "0.6.1"
      ]
    }
  }
}
