{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$md": {
    "name": "qdrant-authentication"
  },
  "type": "object",
  "title": "Qdrant Vector Database",
  "description": "Qdrant database authentication.",
  "required": ["data", "specs"],
  "properties": {
    "data": {
      "type": "object",
      "required": ["infrastructure", "authentication"],
      "properties": {
        "infrastructure": {
          "$ref": "../types/k8s-application-infrastructure.json"
        },
        "authentication": {
          "$ref": "../types/qdrant-authentication.json"
        }
      }
    },
    "specs": {
      "type": "object",
      "required": ["qdrant"],
      "properties": {
        "qdrant": {
          "$ref": "../specs/qdrant.json"
        },
        "aws": {
          "$ref": "../specs/aws.json"
        },
        "azure": {
          "$ref": "../specs/azure.json"
        },
        "gcp": {
          "$ref": "../specs/gcp.json"
        }
      }
    }
  }
}
