{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$md": {
		"access": "public",
		"name": "aws-s3-bucket"
	},
	"type": "object",
	"title": "AWS S3 Bucket",
	"additionalProperties": false,
	"required": [
		"data",
		"specs"
	],
	"properties": {
		"data": {
			"title": "Artifact Data",
			"type": "object",
			"required": [
				"infrastructure"
			],
			"properties": {
				"infrastructure": {
					"title": "Infrastructure",
					"required": [
						"arn"
					],
					"type": "object",
					"properties": {
						"arn": {
							"$ref": "../types/aws-arn.json"
						}
					}
				},
                "security": {
                    "$ref": "../types/aws-security.json"
                }
			}
		},
		"specs": {
			"title": "Artifact Specs",
			"type": "object",
			"properties": {
				"aws": {
					"$ref": "../specs/aws.json"
				}
			}
		}
	}
}
