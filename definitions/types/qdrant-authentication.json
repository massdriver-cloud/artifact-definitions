{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Qdrant Authentication",
  "description": "Qdrant connection string",
  "required": ["hostname", "api_key", "port"],
  "properties": {
    "api_key": {
      "type": "string",
      "$md.sensitive": true
    },
    "hostname": {
      "type": "string"
    },
    "port": {
      "$ref": "./port.json"
    }
  }
}
