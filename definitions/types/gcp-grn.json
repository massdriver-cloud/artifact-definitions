{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "string",
  "title": "GCP Resource Name (GRN)",
  "description": "GCP Resource Name (GRN)",
  "examples": [
    "projects/<my-project>/locations/<location>/instances/<my-instance>",
    "projects/<my-project>/locations/<location>/clusters/<my-cluster>",
    "projects/<my-project>/zones/<zone>/instances/<node-pool-id>"
  ],
  "pattern": "^projects\/[a-z0-9-]*\/[a-z]+\/[a-z0-9-]*\/[a-z0-9-]*\/.*$",
  "message": {
    "pattern": "The provided GRN does not follow the expected pattern. See the examples for valid GRNs."
  }
}
