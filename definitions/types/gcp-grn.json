{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "string",
  "title": "GCP Resource Name (GRN)",
  "description": "GCP Resource Name (GRN)",
  "examples": [
    "projects/my-project/global/networks/my-network",
    "projects/my-project/locations/us-west2/instances/my-instance",
    "projects/my-project/locations/us-west2/clusters/my-cluster",
    "projects/my-project/zones/us-west2-a/instances/node-pool-id"
  ],
  "pattern": "^projects\/[a-z0-9-]+\/[a-z]+\/[a-z0-9-]+\/[a-z0-9-]+.*$",
  "message": {
    "pattern": "The provided GRN does not follow the expected pattern. See the examples for valid GRNs."
  }
}
