{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "string",
  "title": "Azure DFS",
  "description": "Azure DFS",
  "examples": [
    "abfs://filesystem.accountname.dfs.core.windows.net/",
    "http://storageaccount.dfs.core.windows.net/"
  ],
  "pattern": "^((abfs|abfss|http|https):\/\/)([a-zA-Z0-9]+(\\.[a-zA-Z0-9]+)?)\\.dfs\\.core\\.windows\\.net\/$",
  "message": {
    "pattern": "Must be a valid Azure DFS Namespace"
  }
}
