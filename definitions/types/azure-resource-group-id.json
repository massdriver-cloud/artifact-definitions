{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "string",
  "title": "Azure Resource Group ID",
  "description": "Azure Resource Group ID",
  "examples": [
    "/subscriptions/12345678-1234-1234-abcd-1234567890ab/resourceGroups/resource-group-name"
  ],
  "pattern": "^/subscriptions/[a-z0-9]{8}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{12}/resourceGroups/[a-zA-Z0-9-_.()]{0,89}[a-zA-Z0-9-_()]+$",
  "message": {
    "pattern": "Must be a valid Azure Resource Group ID"
  }
}
