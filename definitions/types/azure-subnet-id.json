{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "string",
  "title": "Azure Subnet Resource ID",
  "description": "Azure Subnet Resource ID",
  "examples": [
    "/subscriptions/12345678-1234-1234-abcd-1234567890ab/resourceGroups/resource-group-name/providers/Microsoft.Network/virtualNetworks/network-name/subnets/subnet-name"
  ],
  "pattern": "^\/subscriptions\/[a-z0-9]{8}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{12}\/resourceGroups\/[a-zA-Z0-9-_.()]{0,89}[a-zA-Z0-9-_()]\/providers\/Microsoft\\.[a-zA-Z]+\/[a-zA-Z]+\/[0-9A-Za-z-]+\/subnets\/[a-zA-Z0-9]{1}[a-zA-Z0-9_.-]{0,78}[a-zA-Z0-9_]{1}$",
  "message": {
    "pattern": "Must be a valid Azure Subnet Resource ID"
  }
}
