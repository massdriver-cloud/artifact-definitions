{
    "$md": {
        "importing": {
            "group": "data"
        },
        "name": "env-file"
    },
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "A JSONified list of environment variables.",
    "properties": {
        "data": {
            "properties": {
                "vars": {
                    "items": {
                        "properties": {
                            "key": {
                                "pattern": "^[a-zA-Z_]{1,}[a-zA-Z0-9_]{0,}$",
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            }
                        },
                        "required": [
                            "key",
                            "value"
                        ],
                        "type": "object"
                    },
                    "minItems": 1,
                    "type": "array"
                }
            },
            "required": [
                "vars"
            ],
            "title": "Artifact Data",
            "type": "object"
        },
        "specs": {
            "additionalProperties": true,
            "type": "object"
        }
    },
    "required": [
        "data"
    ],
    "title": "Environment Variables",
    "type": "object"
}
