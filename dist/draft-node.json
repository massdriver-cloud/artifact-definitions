        }
      },
      "required": ["name"],
      "title": "Artifact Data",
      "type": "object"
    },
    "specs": {
      "properties": {},
      "required": [],
      "type": "object"
    }
  },
  "required": ["data", "specs"],
  "title": "Draft Node",
  "type": "object"
}
