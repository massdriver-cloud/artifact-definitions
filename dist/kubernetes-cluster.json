{
    "$md": {
        "export": [
            {
                "downloadButtonText": "Kube Config",
                "fileFormat": "yaml",
                "template": "",
                "templateLang": "liquid"
            }
        ],
        "icon": "https://raw.githubusercontent.com/massdriver-cloud/artifact-definitions/refs/heads/main/definitions/artifacts/kubernetes-cluster/icon.png",
        "label": "Kubernetes Cluster",
        "name": "kubernetes-cluster",
        "ui": {
            "environmentDefaultGroup": "credentials",
            "instructions": [
                {
                    "content": "",
                    "label": "kubectl CLI"
                }
            ]
        }
    },
    "$schema": "http://json-schema.org/draft-07/schema",
    "description": "Kubernetes cluster authentication and cloud-specific configuration.",
    "properties": {
        "data": {
            "description": "Configure your Kubernetes cluster connection details",
            "properties": {
                "authentication": {
                    "description": "Connection details required to authenticate with your Kubernetes cluster",
                    "properties": {
                        "cluster": {
                            "description": "Information about the Kubernetes API server you're connecting to",
                            "properties": {
                                "certificate-authority-data": {
                                    "description": "Base64-encoded certificate authority (CA) certificate used to verify the Kubernetes API server's identity. This ensures you're connecting to the correct cluster.",
                                    "title": "Certificate Authority Certificate",
                                    "type": "string"
                                },
                                "server": {
                                    "description": "The full URL endpoint of your Kubernetes API server (e.g., https://api.example.com:6443)",
                                    "title": "Kubernetes API Server URL",
                                    "type": "string"
                                }
                            },
                            "required": [
                                "server",
                                "certificate-authority-data"
                            ],
                            "title": "Cluster Connection Details",
                            "type": "object"
                        },
                        "user": {
                            "description": "Credentials for authenticating as a user with the Kubernetes cluster",
                            "properties": {
                                "token": {
                                    "$md.sensitive": true,
                                    "description": "Authentication token used to authenticate with the Kubernetes API server. This is typically a service account token or user token.",
                                    "title": "Bearer Token",
                                    "type": "string"
                                }
                            },
                            "required": [
                                "token"
                            ],
                            "title": "User Authentication",
                            "type": "object"
                        }
                    },
                    "required": [
                        "cluster",
                        "user"
                    ],
                    "title": "Cluster Authentication",
                    "type": "object"
                },
                "infrastructure": {
                    "description": "Optional cloud provider-specific configuration for your Kubernetes cluster. Only needed if your cluster is managed by AWS EKS, Azure AKS, or GCP GKE.",
                    "oneOf": [
                        {
                            "$schema": "http://json-schema.org/draft-07/schema",
                            "description": "",
                            "properties": {
                                "arn": {
                                    "$schema": "http://json-schema.org/draft-07/schema",
                                    "description": "Amazon Resource Name",
                                    "examples": [
                                        "arn:aws:rds::ACCOUNT_NUMBER:db/prod",
                                        "arn:aws:ec2::ACCOUNT_NUMBER:vpc/vpc-foo"
                                    ],
                                    "message": {
                                        "pattern": "Correct format of an arn described here: https://docs.aws.amazon.com/general/latest/gr/aws-arns-and-namespaces.html"
                                    },
                                    "pattern": "^arn:aws:[a-zA-Z0-9._-]*:[a-zA-Z0-9._-]*:(?:[0-9]{12})?:[a-zA-Z0-9/:._-]+$",
                                    "title": "AWS ARN",
                                    "type": "string"
                                },
                                "oidc_issuer_url": {
                                    "$schema": "http://json-schema.org/draft-07/schema",
                                    "description": "An HTTPS endpoint URL",
                                    "examples": [
                                        "https://example.com/some/path",
                                        "https://massdriver.cloud"
                                    ],
                                    "pattern": "^https://.*$",
                                    "title": "HTTPS URL",
                                    "type": "string"
                                }
                            },
                            "required": [
                                "arn",
                                "oidc_issuer_url"
                            ],
                            "title": "AWS EKS infrastructure config",
                            "type": "object"
                        },
                        {
                            "$schema": "http://json-schema.org/draft-07/schema",
                            "description": "Azure AKS Infrastructure Configuration",
                            "properties": {
                                "ari": {
                                    "$schema": "http://json-schema.org/draft-07/schema",
                                    "description": "Azure Resource ID",
                                    "examples": [
                                        "/subscriptions/12345678-1234-1234-abcd-1234567890ab/resourceGroups/resource-group-name/providers/Microsoft.Network/virtualNetworks/network-name"
                                    ],
                                    "message": {
                                        "pattern": "Must be a valid Azure Resource ID"
                                    },
                                    "pattern": "^/subscriptions/[a-z0-9]{8}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{4}-[a-z0-9]{12}/resource(g|G)roups/[a-zA-Z0-9-_.()]{0,89}[a-zA-Z0-9-_()]/providers/Microsoft\\.[a-zA-Z]+/[a-zA-Z]+(?:/[a-zA-Z0-9-_.()]+)+$",
                                    "title": "Azure Resource ID",
                                    "type": "string"
                                },
                                "oidc_issuer_url": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "ari"
                            ],
                            "title": "Infrastructure Config",
                            "type": "object"
                        },
                        {
                            "$schema": "http://json-schema.org/draft-07/schema",
                            "description": "Minimal GCP Infrastructure Config",
                            "properties": {
                                "grn": {
                                    "$schema": "http://json-schema.org/draft-07/schema",
                                    "description": "GCP Resource Name (GRN)",
                                    "examples": [
                                        "projects/my-project/global/networks/my-global-network",
                                        "projects/my-project/regions/us-west2/subnetworks/my-subnetwork",
                                        "projects/my-project/topics/my-pubsub-topic",
                                        "projects/my-project/subscriptions/my-pubsub-subscription",
                                        "projects/my-project/locations/us-west2/instances/my-redis-instance",
                                        "projects/my-project/locations/us-west2/clusters/my-gke-cluster"
                                    ],
                                    "message": {
                                        "pattern": "The provided GRN does not follow the expected pattern. See the examples for valid GRNs."
                                    },
                                    "pattern": "^projects/[a-z0-9-]+/[a-z]+/[a-z0-9-]+(?:/[a-zA-Z0-9-_.]+){0,6}$",
                                    "title": "GCP Resource Name (GRN)",
                                    "type": "string"
                                }
                            },
                            "required": [
                                "grn"
                            ],
                            "title": "GCP Infrastructure GRN",
                            "type": "object"
                        }
                    ],
                    "title": "Cloud Provider Information",
                    "type": "object"
                }
            },
            "required": [
                "authentication"
            ],
            "title": "Kubernetes Cluster Configuration",
            "type": "object"
        },
        "specs": {
            "additionalProperties": true,
            "type": "object"
        }
    },
    "required": [
        "data"
    ],
    "title": "Kubernetes Cluster",
    "type": "object"
}
